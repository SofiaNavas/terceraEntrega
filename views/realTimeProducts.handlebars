
<script src="/socket.io/socket.io.js"></script>

<script src="/js/realTimeProducts.js"></script>

<link rel="stylesheet" href="style.css">



<table id="productos">
  <tr>
    <th>ID</th>
    <th>Title</th>
    <th>Description</th>
    <th>Code</th>
    <th>Price</th>
    <th>Status</th>
    <th>Stock</th>
    <th>Category</th>
    
  </tr>
  {{#each products}}
    <tr data-product-id= {{this._id}}>
      <td>{{this._id}}</td>
      <td>{{this.title}}</td>
      <td>{{this.description}}</td>
      <td>{{this.code}}</td>
      <td>{{this.price}}</td>
      <td>{{this.status}}</td>
      <td>{{this.stock}}</td>
      <td>{{this.category}}</td>
      
    </tr>
  {{/each}}
</table>
